<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="21dp"
                android:height="21dp" />
            <stroke
                android:width="2dp"
                android:color="#cbcbcb" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <size
                android:width="11dp"
                android:height="11dp" />
            <solid android:color="#01b99b" />
        </shape>
    </item>
</layer-list>