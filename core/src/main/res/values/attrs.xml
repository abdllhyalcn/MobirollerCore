	<declare-styleable name="twowayview_TwoWayLayoutManager">
		<attr name="android:orientation" tools:ignore="ResourceName"/>
	</declare-styleable>

	<declare-styleable name="MaxHeightScrollView">
		<attr name="maxHeight" format="dimension" />
	</declare-styleable>

